{
    "compilerOptions": {
        "outDir": "./dist",
        "target": "es2016",
        "module": "commonjs",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "resolveJsonModule": true,
        "strict": true,
        "skipLibCheck": true
    },
    "exclude": [
        "**/__tests__/*",
        "dist/*"
    ]
}